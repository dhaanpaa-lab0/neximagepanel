<Window x:Class="NexImagePanel.UMS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NexImagePanel.UMS"
        mc:Ignorable="d"
        Title="User Management Panel" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="User Management" Margin="6" TextAlignment="Center" FontSize="24" FontWeight="Thin"></TextBlock>
        <ListView Margin="6" MinHeight="200" d:ItemsSource="{d:SampleData ItemCount=5}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="User"></GridViewColumn>
                    <GridViewColumn Header="User Type"></GridViewColumn>


                </GridView>
            </ListView.View>
            
        </ListView>
    </StackPanel>
</Window>
